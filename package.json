{
  "name": "ttpt",
  "version": "0.12.3",
  "license": "MIT",
  "bin": {
    "ttpt": "./lib/index.js"
  },
  "scripts": {
    "baseTest": "rm -rf demo && mkdir demo && cd demo && tsx ../src/index.ts -e",
    "lint": "tsc --noEmit --target ESNext --moduleResolution NodeNext --module NodeNext ./src/*.ts && eslint --fix '**/*.{ts,tsx,js,jsx}' && prettier --write . && sort-package-json",
    "prepublishOnly": "rm -rf lib && tsc src/index.ts --outDir lib",
    "test": "rm -rf demo && mkdir demo && cd demo && tsx ../src/index.ts -w"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "shell-commands": "^0.4.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "yarn-upgrade-all": "^0.7.4"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "eslint"
    ]
  }
}
