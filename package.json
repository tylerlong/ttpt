    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.10.0",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "yarn-upgrade-all": "^0.7.2"
